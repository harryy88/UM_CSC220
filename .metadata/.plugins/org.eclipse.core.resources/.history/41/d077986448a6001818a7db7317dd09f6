package lab01;

public class CoinFlipExperiment {
	public static void main(String[] args) {
		int amount = 0; 
		for (int i=0; i < 100; i++) {
			double flip = Math.random();
			if (flip < 0.505) {
				amount += 1; 
			} 
			else {
				amount -= 1; 
			}
		}
		System.out.println("You Total Winning is: " + amount); 
	}
		
}
